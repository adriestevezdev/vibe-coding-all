# Environment
PYTHON_ENV=development
# Application Settings
APP_NAME="Vibe Coding API"
APP_VERSION="1.0.0"
DEBUG=false

# Server Settings
HOST=0.0.0.0
PORT=8000

# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/vibe_coding
