# Backend environment variables
DATABASE_URL=postgresql://postgres:postgres@db:5432/vibe_coding
SECRET_KEY=your-secret-key-here-change-in-production
JWT_SECRET_KEY=your-jwt-secret-key-here-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_DELTA=86400

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here

# Polar.sh Payments
POLAR_ACCESS_TOKEN=polar_oat_sandbox_your_token_here
POLAR_WEBHOOK_SECRET=whsec_your_webhook_secret_here
POLAR_ORGANIZATION_ID=your_organization_id_here
